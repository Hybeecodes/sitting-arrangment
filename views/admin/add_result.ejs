<!DOCTYPE html>
<html lang="en">

<% include ./partials/head %>

<body class="animsition">
    <div class="page-wrapper">
        <!-- HEADER MOBILE-->
        <% include ./partials/nav_mobile %>
        <!-- END HEADER MOBILE-->
        <!-- MENU SIDEBAR-->
        <% include ./partials/sidebar %>
        <!-- END MENU SIDEBAR-->
        <!-- PAGE CONTAINER-->
        <div class="page-container">
            <!-- HEADER DESKTOP-->
            <% include ./partials/nav %>
            <!-- HEADER DESKTOP-->
            <!-- MAIN CONTENT-->
            <div class="main-content">
                <div class="section__content section__content--p30">
                    <div class="container-fluid">
                        <div class="row">
                        </div>
                        <div class="container">
                            <div class="col-lg-12">
                                    <h2 class="title-1 m-b-25">Add Student Result</h2>
                                    <form action="" id="enrollStudentForm">
                                            <div class="form-group">
                                                <label for="students">Student</label>
                                                <select name="student" multiple id="student" class="form-control">
                                                    <option value="">Select Student</option>
                                                </select>
                                            </div>

                                            <div class="form-group">
                                                <label for="course">Course</label>
                                                <select name="course" id="course" class="form-control">
                                                    <option value="">Select Course</option>
                                                </select>
                                            </div>

                                        <div class="form-group">
                                            <label for="score">Exam Score</label>
                                            <input type="score" min="0" name="score" id="score" placeholder="Score" class="form-control">
                                        </div>

                                        
                                        
                                        <button type="submit" class="btn btn-primary">Add Student Result</button>
                                    </form>
                                </div>
                            </div>
                        </div>
                        <% include ./partials/footer %>
                    </div>
                </div>
            </div>
            <!-- END MAIN CONTENT-->
            <!-- END PAGE CONTAINER-->
        </div>

    </div>

    <% include ./partials/script %>
    <script>
        $('#course').select2({
            placeholder: "Select Course",
            width: "100%"
        })

        $('#student').select2({
            placeholder: "Select Student",
            width: "100%"
        })
    </script>
</body>

</html>
<!-- end document-->
